{
  "name": "react-ts-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "fix": "eslint --fix --ext .ts,.js,.tsx,.jsx ./src",
    "prepare": "husky install",
    "changelog": "conventional-changelog -p react -i CHANGELOG.md -s -r 0",
    "commit": "git-cz"
  },
  "lint-staged": {
    "src/**/*.scss": "stylelint --syntax sass",
    "src/**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "src/**/*.{js,jsx,tsx,ts,less,md,json}": ["prettier --write"]
  },
  "dependencies": {
    "@types/node": "^18.11.17",
    "ahooks": "^3.7.3",
    "antd": "^5.1.0",
    "dayjs": "^1.11.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.6.1",
    "react-router-dom": "^6.6.1",
    "umi-request": "^1.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@iconify-json/carbon": "^1.1.11",
    "@iconify/json": "^2.1.156",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@unocss/reset": "^0.48.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "commitizen": "^4.2.6",
    "conventional-changelog": "^3.1.25",
    "conventional-changelog-cli": "^2.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.30.0",
    "eslint-config-alloy": "^4.7.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "git-cz": "^4.9.0",
    "husky": "^8.0.2",
    "lint-staged": "^11.1.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.8.1",
    "prettier-eslint": "^15.0.1",
    "sass": "^1.57.1",
    "typescript": "^4.9.3",
    "unocss": "^0.48.0",
    "unplugin-auto-import": "^0.12.1",
    "vite": "^4.0.0",
    "vite-plugin-pages": "^0.28.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
